       try {\r\n            int res = userMapper.getHost(username);\r\n            System.out.println(res);\r\n            return res;\r\n        } catch (Exception e) {\r\n            e.printStackTrace();\r\n        }\r\n        return 0;\r\n    }\r\n\r\n    @Override\r\n    public User getUserDetail(int uId) {\r\n        return userMapper.getUserDetail(uId);\r\n    }\r\n\r\n\r\n}\r\n
